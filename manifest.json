{
	"manifest_version": 2,
	"icons": {
		"48": "image/icon.png",
		"128": "image/icon128.png"
	},
	"name": "attain.ai",
	"version": "1.0",
	"applications": {
		"gecko": {
			"id": "catgifs@mozilla.org"
		}
	},
	"background": {
		"scripts": ["./js/background.js"],
		"persistent": false
	},
	"permissions": ["tabs", "notifications", "http://*/", "https://*/"],
	"browser_action": {
		"default_icon": {
			"19": "image/button.png",
			"38": "image/button38.png"
		},
		"default_title": "attain.ai"
	}
}